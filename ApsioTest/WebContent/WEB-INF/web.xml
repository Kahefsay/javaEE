<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ApsioTest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>toto</servlet-name>
    <servlet-class>com.formation.servlets.Premiere</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>toto</servlet-name>
    <url-pattern>/toto</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deuxieme</servlet-name>
    <servlet-class>com.formation.servlets.Deuxieme</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deuxieme</servlet-name>
    <url-pattern>/Page2</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/maTagLib.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
</web-app>